GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/AFNetworking" "${PODS_ROOT}/Headers/Public/BFOAuth" "${PODS_ROOT}/Headers/Public/CloudSight" "${PODS_ROOT}/Headers/Public/RequestUtils" "${PODS_ROOT}/Headers/Public/mopub-ios-sdk"
LIBRARY_SEARCH_PATHS = $(inherited) "$PODS_CONFIGURATION_BUILD_DIR/AFNetworking" "$PODS_CONFIGURATION_BUILD_DIR/BFOAuth" "$PODS_CONFIGURATION_BUILD_DIR/CloudSight" "$PODS_CONFIGURATION_BUILD_DIR/RequestUtils" "$PODS_CONFIGURATION_BUILD_DIR/mopub-ios-sdk"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/AFNetworking" -isystem "${PODS_ROOT}/Headers/Public/BFOAuth" -isystem "${PODS_ROOT}/Headers/Public/CloudSight" -isystem "${PODS_ROOT}/Headers/Public/RequestUtils" -isystem "${PODS_ROOT}/Headers/Public/mopub-ios-sdk"
OTHER_LDFLAGS = $(inherited) -ObjC
PODS_BUILD_DIR = $BUILD_DIR
PODS_CONFIGURATION_BUILD_DIR = $PODS_BUILD_DIR/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_ROOT = ${SRCROOT}/Pods
